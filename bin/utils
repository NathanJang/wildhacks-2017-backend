#!/usr/bin/env node

const config = function() {
    return new Promise(function(resolve, reject) {
        try {
            const sequelize = require('../config/sequelize.json');
            resolve(sequelize);
        } catch(err) {
            reject(new Error('Cannot import sequelize file at /config/sequelize.json'));
        }

    });
}

module.exports = {
    config
};